pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                checkout([$class: 'GitSCM', branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/SmokyNight/spring-petclinic.git']]])
            }
        }
        stage('Build') {
            steps {
                sh './gradlew build'
            }
        }
        
    }
}
